@media all and (max-width: 700px) and (min-width: 0px)
	#menu
		width 100%
		max-height 50px
		min-height 50px
		display block
		.logo
			width 87px
			height 50px
			background-size 70%
			margin-left 10px
			background-position center left
		#menu-mob
			display block
			background-image url('img/menu.png')
			background-position center
			background-repeat no-repeat
			background-size 30px
			width 50px
			height 50px
			float right
			margin-right 5px
		.holder
			display none
			width 100%
			position absolute
			top 40px
			left -1% 
			height 30%
			text-align center
			z-index 9999
			a 
				width 100%
				padding-top 20px
				padding-bottom 20px
				color DarkRed
				border-bottom 2px solid DarkRed
				border-top 2px solid DarkRed
				background-color #fff
				padding-right 30px
				&:hover
					text-decoration none
					background-color Boones

	#section1
		.texto
			width 90%
			margin-left 5%
			top 10%
		.box
			left 0%
	#section2
		#gallery
			position absolute
			bottom 0px
			a 
				width 100%
				height 100%
			&:after
				content url(img/next.png)
				position absolute
				z-index 1
				right 5px
				top 45%
				&:hover
					opacity .4
			&:before 
				content url(img/prev.png)
				position absolute
				z-index 1
				left 5px
				top 45%
				&:hover
					opacity .4
			.pic 
				display block
				width 50%
				height 100%
			.do-not
				display none
	#pp-nav span 
		display none
	#section4
		#text
			.box
				display block
				float none
				width 100%
				span 
					margin-top 0%
