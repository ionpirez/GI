@import 'reset.css'
@import 'jquery.pagepiling.css'
@import url(http://fonts.googleapis.com/css?family=Raleway);
@import 'lightbox.css'
box-shadow()
	-webkit-box-shadow arguments 
	-moz-box-shadow arguments
	box-shadow arguments
DarkRed = #a70000
DarkGrey = #444444
Boones = #ECECEC
*
	font-family 'Pontano Sans', sans-serif
#menu
	width 100%
	min-height 70px
	z-index 77
	display  block
	position fixed
	background-color #fff
	border-bottom 2px solid DarkRed
	box-shadow  -3px 3px 1px 0px rgba(167, 0, 0, 0.07)
	.logo
		width 300px
		position absolute
		height 100%
		z-index 90
		background-image url('img/kajile.png')
		background-repeat no-repeat
		background-position center
		background-size 30%
	#menu-mob
		display none
	.holder
		width 70%
		float right
		height 100%
		font-size 0px
		z-index 90
		margin-top 1.5%
		height 65%
		text-align right
		margin-right 30px
		a
			font-size 16px
			display inline-block
			vertical-align middle
			margin-left 1%
			color DarkGrey
			&:hover
				text-decoration underline
				color DarkRed
#section1
	.texto 
		position absolute
		width 50%
		margin-left 25%
		top 40%
		background-color rgba(0, 0, 0, .05)
		z-index 70
		display block
		min-width 300px
		box-shadow  -3px 8px 8px 4px rgba(255, 255, 255, 0.07)
		h1
			z-index 77
			color #fff
			text-shadow 2px 2px black
		p 
			color #fff
			z-index 77
			text-shadow 2px 2px black
	.box 
		width 100px
		min-width 70px
		height 110px
		position absolute
		bottom 0px
		left 47%
		background-color black
		background-color rgba(167, 0, 0, .8)
		z-index 70
		background-image url('img/arrow.png')
		background-repeat no-repeat
		background-size 30%
		background-position center !important
	#slider
		background-size cover
		background-repeat no-repeat
		background-position center
		width 100%
		height 100%
#section2
	.text 
		color black
		width 80%
		margin 0px
		margin-left 10%
		height auto
		position absolute
		top 150px
		h1
			color black
			text-shadow 2px 2px #fff
			marign 0px
			font-size 3vw
		p 
			color black
			text-shadow 2px 2px #fff
			marign 0px
			font-size 1vw
		span 
			&:after, &:before
				content url('img/border.png')
	#gallery
		width 100%
		background-color rgba(255,255,255, .3)
		position absolute
		height 45%
		bottom 0px
		.pic 
			width 25%
			float left
			height 50%
			display inline-block
			font-size 0px
			vertical-align middle
			background-position center
			background-size cover
	.hover-grid 
		.hover-grid-item 
			width  100%
			height  100%
			overflow hidden
			position relative
			cursor default
			.caption 
				background-color  rgba(44,44,44,.5)
				width 100%
				height 100%
				color  #fff
				display none
				p 
					position absolute
					top 35%
					font-size  15px
					text-shadow 2px 2px DarkGrey
					color  #fff
					left 2%
#section3
	#building
		margin 100px auto
		width 487px
		height 534px
		#inner
			position relative
			height 100%
			width 100%
			.full
				 width 100%
				 height 14.28%
				 display inline-block
				 background-color red
				 font-size 0px
				 margin 0px
				 padding 0px
				 float left
				 text-align center
				 vertical-align center
			.medium 
				 width 50%
				 height 14.28%
				 display inline-block
				 font-size 0px
				 margin 0px
				 padding 0px
				 float left
				 text-align center
				 vertical-align center
				 background-repeat no-repeat
				 background-size cover
				 background-position center left
				 strong 
